&pm8226_mpps {
	mpp@a600 { /* MPP 7 */
		/* PA_THERM1 config */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <2>; /* AMUX 7 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};

	mpp@a100 { /* MPP 2 */
		/* PA_THERM1 config */
		qcom,mode = <4>; /* AIN input */
		qcom,invert = <1>; /* Enable MPP */
		qcom,ain-route = <1>; /* AMUX 6 */
		qcom,master-en = <1>;
		qcom,src-sel = <0>; /* Function constant */
	};
};

&pm8226_vadc {
	pa_mpp_number = <0x14>;
	pa1_mpp_number = <0x16>;
	cpu_mpp_number = <0x11>;

	chan@16 {
		label = "pa_therm1";
		reg = <0x16>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <8>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@11 {
		label = "cpu_therm";
		reg = <0x11>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <8>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
	};

	chan@14 {
		qcom,scale-function = <0x8>;
	};
	
	chan@17 {
		label = "pa_therm_NA";
	};	

};

&pm8226_adc_tm {
	chan@14 {
		label = "pa_therm0";
		reg = <0x14>;
		qcom,decimation = <0x0>;
		qcom,pre-div-channel-scaling = <0x0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <0x8>;
		qcom,hw-settle-time = <0x2>;
		qcom,fast-avg-setup = <0x0>;
		qcom,btm-channel-number = <0x78>;
		qcom,thermal-node;
	};

	chan@16 {
		label = "pa_therm1";
		reg = <0x16>;
		qcom,decimation = <0>;
		qcom,pre-div-channel-scaling = <0>;
		qcom,calibration-type = "ratiometric";
		qcom,scale-function = <8>;
		qcom,hw-settle-time = <2>;
		qcom,fast-avg-setup = <0>;
		qcom,btm-channel-number = <0x7a>;
		qcom,thermal-node;
	};

	chan@17 {
		label = "pa_therm_NA";
	};	

};

/ {
        boardid_info {
                  compatible = "huawei,hw_boardid_info";
                  boardid = [18];
                  status = "ok";
        };
};
